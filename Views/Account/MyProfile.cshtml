@model EFDbFirstApproachExample.Identity.ApplicationUser
@{
    ViewBag.Title = "MyProfile";
}

<h2>My Profile</h2>
<table class="table-bordered">
    <tr>
        <th>Username</th>
        <td>@Model.UserName</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Mobile</th>
        <td>@Model.PhoneNumber</td>
    </tr>
    <tr>
        <th>Date Of Birth</th>
        <td>@Model.Birthday</td>
    </tr>
    <tr>
        <th>City</th>
        <td>@Model.City</td>
    </tr>
    <tr>
        <th>Country</th>
        <td>@Model.Country</td>        
    </tr>
</table>

<a href="/home/index" class="btn btn-secondary">Back</a>


